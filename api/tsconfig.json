{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "outDir": "build/"
  }
}
